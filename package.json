{
  "name": "npm-browser",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "tmux new-session -d -s \"npm-browser\" node ./server/index.js",
    "stop": "tmux kill-session -t \"npm-browser\"",
    "bring": "tmux attach-session -t \"npm-browser\"",
    "build:routes": "./scripts/build-routes-client.ts > src/routes.generated.tsx",
    "build": "./scripts/build-production.sh",
    "dev": "NODE_ENV=development EXECUTE=1 ./scripts/build-server.ts",
    "deploy": "./scripts/deploy.sh"
  },
  "dependencies": {
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^8.0.3",
    "@fastify/static": "^6.11.2",
    "fastify": "^4.24.3"
  },
  "devDependencies": {
    "@aet/eslint-rules": "0.0.1-beta.42",
    "@aet/icons": "0.0.1-beta.7",
    "@blueprintjs/core": "^5.5.1",
    "@blueprintjs/icons": "^5.2.1",
    "@blueprintjs/select": "^5.0.16",
    "@codemirror/lang-javascript": "^6.2.1",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/language": "^6.9.2",
    "@codemirror/search": "^6.5.4",
    "@codemirror/state": "^6.3.1",
    "@codemirror/view": "^6.21.4",
    "@emotion/babel-plugin": "^11.11.0",
    "@emotion/css": "11.11.2",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@fastify/middie": "^8.3.0",
    "@lezer/highlight": "^1.1.6",
    "@react-hookz/web": "^23.1.0",
    "@tanstack/react-query": "^5.0.5",
    "@types/debug": "^4.1.10",
    "@types/lodash": "^4.14.200",
    "@types/node": "^20.8.9",
    "@types/nodemon": "^1.19.4",
    "@types/react": "18.2.32",
    "@types/react-dom": "18.2.14",
    "@types/validate-npm-package-name": "^4.0.1",
    "@uiw/codemirror-theme-github": "^4.21.20",
    "@uiw/react-codemirror": "^4.21.20",
    "@uiw/react-markdown-preview": "^4.1.16",
    "@vitejs/plugin-react": "^4.1.0",
    "babel-plugin-macros": "^3.1.0",
    "debug": "^4.3.4",
    "esbuild": "0.19.5",
    "eslint": "8.52.0",
    "fast-glob": "^3.3.1",
    "fastify-plugin": "^4.5.1",
    "json5": "^2.2.3",
    "lodash": "^4.17.21",
    "nodemon": "^3.0.1",
    "normalize.css": "^8.0.1",
    "path": "^0.12.7",
    "prettier": "^3.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-paginate": "^8.2.0",
    "tiny-invariant": "^1.3.1",
    "tiny-lru": "^11.2.3",
    "typescript": "5.2.2",
    "validate-npm-package-name": "^5.0.0",
    "vite": "^4.5.0",
    "vite-tsconfig-paths": "^4.2.1",
    "wouter": "^2.12.1"
  },
  "pm2": {
    "apps": [
      {
        "name": "npm-browser",
        "script": "node",
        "args": "./server/index.js",
        "env": {
          "PORT": 3008
        }
      }
    ]
  }
}
