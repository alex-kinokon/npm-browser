{
  "name": "npm-browser",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "tmux new-session -d -s \"npm-browser\" node ./server/index.mjs",
    "stop": "tmux kill-session -t \"npm-browser\"",
    "bring": "tmux attach-session -t \"npm-browser\"",
    "build:routes": "./scripts/build-routes-client.ts > src/routes.generated.tsx",
    "build": "./scripts/build-production.sh",
    "dev": "NODE_ENV=development EXECUTE=1 ./scripts/build-server.ts",
    "deploy": "./scripts/deploy.sh"
  },
  "dependencies": {
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^9.0.1",
    "@fastify/static": "^6.12.0",
    "fastify": "^4.25.2"
  },
  "devDependencies": {
    "@aet/eslint-rules": "0.0.9",
    "@aet/icons": "0.0.1-beta.10",
    "@blueprintjs/core": "^5.7.2",
    "@blueprintjs/icons": "^5.5.0",
    "@blueprintjs/select": "^5.0.20",
    "@codemirror/lang-javascript": "^6.2.1",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/language": "^6.10.0",
    "@codemirror/search": "^6.5.5",
    "@codemirror/state": "^6.4.0",
    "@codemirror/view": "^6.23.0",
    "@emotion/babel-plugin": "^11.11.0",
    "@emotion/css": "11.11.2",
    "@emotion/react": "11.11.3",
    "@emotion/styled": "11.11.0",
    "@fastify/middie": "^8.3.0",
    "@jspm/core": "^2.0.1",
    "@lezer/highlight": "^1.2.0",
    "@monaco-editor/react": "^4.6.0",
    "@react-hookz/web": "^24.0.2",
    "@tanstack/react-query": "^5.15.0",
    "@types/debug": "^4.1.12",
    "@types/is-core-module": "^2.2.2",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.5",
    "@types/nodemon": "^1.19.6",
    "@types/react": "18.2.46",
    "@types/react-dom": "18.2.18",
    "@types/validate-npm-package-name": "^4.0.2",
    "@uiw/codemirror-theme-github": "^4.21.21",
    "@uiw/react-codemirror": "^4.21.21",
    "@uiw/react-markdown-preview": "^5.0.6",
    "@vitejs/plugin-react": "^4.2.1",
    "babel-plugin-macros": "^3.1.0",
    "debug": "^4.3.4",
    "es-module-lexer": "^1.4.1",
    "esbuild": "0.19.10",
    "eslint": "8.56.0",
    "fast-glob": "^3.3.2",
    "fastify-plugin": "^4.5.1",
    "is-core-module": "^2.13.1",
    "json5": "^2.2.3",
    "lodash": "^4.17.21",
    "monaco-editor": "^0.45.0",
    "monaco-editor-auto-typings": "^0.4.5",
    "nodemon": "^3.0.2",
    "normalize.css": "^8.0.1",
    "path": "^0.12.7",
    "prettier": "^3.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-paginate": "^8.2.0",
    "tiny-invariant": "^1.3.1",
    "tiny-lru": "^11.2.5",
    "typescript": "5.3.3",
    "validate-npm-package-name": "^5.0.0",
    "vite": "^5.0.10",
    "vite-tsconfig-paths": "^4.2.3",
    "wouter": "^2.12.1"
  },
  "pm2": {
    "apps": [
      {
        "name": "npm-browser",
        "script": "node",
        "args": "./server/index.js",
        "env": {
          "PORT": 3008
        }
      }
    ]
  }
}
